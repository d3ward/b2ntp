{
  "name": "b2ntp",
  "version": "2.0.0",
  "description": "Bring your bookmarks to the homepage with a customizable Kanban style board, tailor your browsing experience to fit your flow",
  "scripts": {
    "purge": "node config/purgecss.js",
    "purge-ext": "node config/purgecss-ext.js",
    "dev": "webpack serve --config config/webpack.dev.js",
    "dev-ext": "cross-env TYPE=chrome MV=3 webpack --config config/webpack.ext.dev.js",
    "dev-ext2": "cross-env TYPE=chrome MV=2 webpack --config config/webpack.ext.dev.js",
    "dev-firefox": "cross-env TYPE=firefox MV=3 webpack --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "build-ext2": "cross-env TYPE=chrome MV=2 webpack --config config/webpack.ext.prod.js",
    "build-ext": "cross-env TYPE=chrome MV=3 webpack --config config/webpack.ext.prod.js",
    "build-ext-f2": "cross-env TYPE=firefox MV=2 webpack --config config/webpack.ext.prod.js",
    "build-ext-f": "cross-env TYPE=firefox MV=3 webpack --config config/webpack.ext.prod.js",
    "lint": "eslint  'src/**/*.{js,ts}' && echo 'Linting complete!'",
    "lint-fix": "eslint  'src/**/*.{js,ts}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3ward/b2ntp.git"
  },
  "author": "Eduard Ursu",
  "license": "(CC BY-NC-SA 4.0)",
  "bugs": {
    "url": "https://github.com/d3ward/b2ntp/issues"
  },
  "homepage": "https://d3ward.github.io/b2ntp",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "6.2.0",
    "fs-extra": "11.2.0",
    "glob": "^11.0.0",
    "glob-all": "^3.3.1",
    "html-loader": "5.1.0",
    "html-webpack-plugin": "5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.40",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.0",
    "prettier": "^3.3.3",
    "purgecss-webpack-plugin": "^6.0.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "4.0.0",
    "template-ejs-loader": "^0.9.4",
    "url-loader": "^4.1.1",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-merge": "^6.0.1",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "a11y-dialog": "^8.0.4",
    "croppie": "^2.6.5",
    "embla-carousel": "^8.1.8",
    "embla-carousel-autoplay": "^8.1.8",
    "interactjs": "^1.10.27",
    "markdown-it": "^14.1.0",
    "marked": "^13.0.3",
    "sortablejs": "^1.15.2",
    "vanilla-picker": "^2.12.3"
  }
}
